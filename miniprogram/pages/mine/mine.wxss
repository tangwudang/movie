/* pages/mine/mine.wxss */

.swiper-tab {
  display: flex;
  width: 100%;
  height: 200rpx;
  justify-content: space-between;
  align-items: center;
}

.swiper-tab-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.swiper-active{
  color: orange;
}


.back{
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  height: 100rpx;
  width: 100%;
  background: white;
  bottom: 0;
  size: 36rpx;
  color: gray
}